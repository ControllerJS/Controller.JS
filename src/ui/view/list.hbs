<frame posn="0 0 0">

  <quad posn="0 0 0" sizen="220 130" bgcolor="FFFA" halign="center" valign="center" style="Bgs1" substyle="NavButtonBlink"/>
  <quad posn="-109 64 0" sizen="10 10" bgcolor="FFFA" style="Icons64x64_1" substyle="Browser"/>
  <label posn="0 61.5 0" sizen="190 7" text="{{ title }}" textsize="5" textcolor="FFFFFFA" halign="center"/>

  <quad posn="99 64 0" sizen="10 10" bgcolor="FFFA" style="Icons64x64_1" substyle="Close" action="core_button_close"/>

  <quad posn="-110 54 0" sizen="220 1" bgcolor="FFFA" style="Bgs1" substyle="ProgressBarSmall"/>

  <frame posn="-105 50 0" id="core_list_frame">
    <frame posn="0 0 0" id="core_list_header">
      <quad posn="0 0 0" sizen="210 8" bgcolor="0008"/>

      {{#each header}}
        <label posn="{{ this.left }} -2 0" sizen="{{ this.width }} 6" text="{{ this.name }}" textsize="3" />
      {{/each}}

    </frame>

    <frame posn="0 -8 0" id="core_list_content">
      <quad posn="0 0 0" sizen="210 90" bgcolor="0002"/>

      {{#each body}}
        <frame posn="0 -{{ this.top }} 0">
          {{#if this.even}}
              <quad posn="0 0 0" sizen="210 6" bgcolor="0004"/>
          {{^}}
              <quad posn="0 0 0" sizen="210 6" bgcolor="0003"/>
          {{/if}}

          {{#each this.data}}
            {{#if this.custom}}
                {{this.custom}}
            {{else if this.button}}
                <quad posn="{{ this.left }} 0 0" sizen="6 6" style="{{ this.style }}" substyle="{{ this.substyle }}" bgcolor="FFF0" {{ this.event }} />
            {{else}}
                <label posn="{{ this.left }} -2 0" sizen="{{ this.width }} 4" textsize="1.9" text="{{ this.text }}" {{ this.action }} />
            {{/if}}
          {{/each}}
        </frame>
      {{/each}}
    </frame>


    <frame posn="0 -102 0">

      <!-- <label text="&#0x;" /> -->
      <label posn="0 -0 0" sizen="72 5" text="Items {{ range.start }}-{{ range.stop }}, Page {{ page }}/{{ pages }}" textsize="2" autonewline="1"/>
      <label posn="0 -5 0" sizen="72 5" text="Total {{ total }} items" textsize="2" autonewline="1"/>

      {{#if prev}}
          <quad posn="150 0 0" sizen="10 10" bgcolor="FFFA" style="Icons64x64_1" substyle="ArrowFirst" action="core_button_first" />
          <quad posn="160 0 0" sizen="10 10" bgcolor="FFFA" style="Icons64x64_1" substyle="ArrowFastPrev" action="core_button_prev_10" />
          <quad posn="170 0 0" sizen="10 10" bgcolor="FFFA" style="Icons64x64_1" substyle="ArrowPrev" action="core_button_prev" />
      {{else}}
          <quad posn="150 0 0" sizen="10 10" bgcolor="FFFA" style="Icons64x64_1" substyle="ArrowFirst" opacity="0.2" />
          <quad posn="160 0 0" sizen="10 10" bgcolor="FFFA" style="Icons64x64_1" substyle="ArrowFastPrev" opacity="0.2" />
          <quad posn="170 0 0" sizen="10 10" bgcolor="FFFA" style="Icons64x64_1" substyle="ArrowPrev" opacity="0.2" />
      {{/if}}

      {{#if next}}
          <quad posn="180 0 0" sizen="10 10" bgcolor="FFFA" style="Icons64x64_1" substyle="ArrowNext" action="core_button_next" />
          <quad posn="190 0 0" sizen="10 10" bgcolor="FFFA" style="Icons64x64_1" substyle="ArrowFastNext" action="core_button_next_10" />
          <quad posn="200 0 0" sizen="10 10" bgcolor="FFFA" style="Icons64x64_1" substyle="ArrowLast" action="core_button_last" />
      {{else}}
          <quad posn="180 0 0" sizen="10 10" bgcolor="FFFA" style="Icons64x64_1" substyle="ArrowNext" opacity="0.2" />
          <quad posn="190 0 0" sizen="10 10" bgcolor="FFFA" style="Icons64x64_1" substyle="ArrowFastNext" opacity="0.2" />
          <quad posn="200 0 0" sizen="10 10" bgcolor="FFFA" style="Icons64x64_1" substyle="ArrowLast" opacity="0.2" />
      {{/if}}

      <quad posn="0 0 0" sizen="210 10" bgcolor="F900"/>

    </frame>
  </frame>
</frame>
