{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "sourceMap": true,
    "declaration": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "isolatedModules": false,
    "noLib": false,
    "suppressImplicitAnyIndexErrors": false
  },
  "exclude": [
    "node_modules",
    "type",
    "typings"
  ],
  "compileOnSave": true,
  "atom": {
    "rewriteTsconfig": false
  }
}
